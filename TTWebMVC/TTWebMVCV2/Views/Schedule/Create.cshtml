@model ScheduleDefViewModel
@{
    var formButtonViewModel = new FormButtonViewModel(createUrl: Url.Action("Create"), returnUrl: Url.Action("Index"));
}
@using (Html.BeginForm("create", "schedule", FormMethod.Post, new { @id = "createScheduleForm" }))
{
    <fieldset>
        @Html.EditorForModel()
        <partial name="_FormButtonsPartial" model="formButtonViewModel" />
    </fieldset>
}